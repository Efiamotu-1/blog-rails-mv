<div class='user_box' >
  <div class='user_photo'>        

 <%= image_tag( 'https://icons.iconarchive.com/icons/iconsmind/outline/512/User-icon.png', size: "50x50", :alt => 'User: ' + user.name.to_s  ) -%> 
  </div>
  <div class='user_namePost'>
      <div class='user_name'>
        <h1><%= link_to user.name, user%></h1>
      </div>
